<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Outer circle -->
  <circle cx="100" cy="100" r="90" fill="#0076CE" />
  
  <!-- Inner circle -->
  <circle cx="100" cy="100" r="70" fill="#1A1A2E" />
  
  <!-- Globe grid lines -->
  <path d="M30,100 A70,70 0 0,1 170,100" stroke="#FF7900" stroke-width="2" fill="none" />
  <path d="M40,60 A70,70 0 0,1 160,60" stroke="#FF7900" stroke-width="2" fill="none" />
  <path d="M40,140 A70,70 0 0,1 160,140" stroke="#FF7900" stroke-width="2" fill="none" />
  
  <path d="M100,30 A70,70 0 0,1 100,170" stroke="#FF7900" stroke-width="2" fill="none" />
  <path d="M60,40 A70,70 0 0,1 60,160" stroke="#FF7900" stroke-width="2" fill="none" />
  <path d="M140,40 A70,70 0 0,1 140,160" stroke="#FF7900" stroke-width="2" fill="none" />
  
  <!-- Nodes representing suppliers -->
  <circle cx="120" cy="70" r="5" fill="#28a745" />
  <circle cx="80" cy="60" r="5" fill="#28a745" />
  <circle cx="140" cy="110" r="5" fill="#ffc107" />
  <circle cx="60" cy="120" r="5" fill="#dc3545" />
  <circle cx="100" cy="140" r="5" fill="#28a745" />
  
  <!-- Radar sweep effect -->
  <path d="M100,100 L170,60" stroke="#0076CE" stroke-width="2" fill="none" opacity="0.7">
    <animateTransform 
      attributeName="transform" 
      attributeType="XML" 
      type="rotate" 
      from="0 100 100" 
      to="360 100 100" 
      dur="4s" 
      repeatCount="indefinite" />
  </path>
  
  <!-- Pulse effect around the logo -->
  <circle cx="100" cy="100" r="85" fill="none" stroke="#FF7900" stroke-width="2" opacity="0.5">
    <animate 
      attributeName="r" 
      values="75;85;75" 
      dur="2s" 
      repeatCount="indefinite" />
    <animate 
      attributeName="opacity" 
      values="0.8;0.2;0.8" 
      dur="2s" 
      repeatCount="indefinite" />
  </circle>
</svg>
